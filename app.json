{
  "expo": {
    "name": "pizdatijApp",
    "slug": "pizdatijApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/apple-touch-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/apple-touch-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#013220"
    },
    "notification":{
      "icon":"./src/assets/images/apple-touch-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akotikovs.pizdatijApp",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/apple-touch-icon.png",
        "backgroundColor": "#013220"
      },
      "useNextNotificationsApi": true,
      "package": "com.akotikovs.pizdatijApp"
    },
    "web": {
      "favicon": "./src/assets/images/favicon-16x16.png"
    }
  }
}
